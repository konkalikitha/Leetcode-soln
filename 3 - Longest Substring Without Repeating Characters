class Solution:
    def lengthOfLongestSubstring(self, s: str) -> int:
        mx = 0
        left = 0
        ch = set()
        curr_len = 0
        for right in range (len(s)):
            while s[right] in ch:
                ch.remove(s[left])
                left+=1
                curr_len -= 1
            ch.add(s[right])    
            curr_len += 1
            mx = max(mx, curr_len)
        return mx
